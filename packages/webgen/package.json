{
  "name": "@pritt/webgen",
  "description": "Generate the web application for Pritt based on presets (might not do this in dart again...)",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "compile": "dart compile js bin/webgen.dart --server-mode --enable-asserts -o dist/webgen_dart.js",
    "bundle": "esbuild bin/webgen.ts --bundle --minify --format=esm --platform=node --outfile=dist/webgen.js",
    "build": "pnpm compile && pnpm bundle",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/chroma-js": "^3.1.1",
    "@types/node": "^22.15.21",
    "esbuild": "0.25.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@babel/generator": "^7.27.3",
    "@babel/template": "^7.27.2",
    "@babel/types": "^7.27.3",
    "chroma-js": "^3.1.2",
    "handlebars": "^4.7.8",
    "zod": "^3.25.30"
  }
}
