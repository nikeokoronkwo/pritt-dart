{
  "name": "@pritt/workspace",
  "private": "true",
  "description": "",
  "type": "module",
  "scripts": {
    "cli": "dart cli/bin/pritt.dart",
    "schema": "pkl eval config/main.pkl --project-dir config -f yaml -o config/out/openapi.yaml && pkl eval config/main.pkl --project-dir config -f json -o config/out/openapi.json",
    "format": "pnpm format:dart && pnpm format:js",
    "format:dart": "dart format .",
    "format:js": "prettier . --write",
    "server": "./tools/denv.dart --cwd server -- dart run bin/server.dart",
    "common-gen": "./scripts/openapigen ./config/out/openapi.yaml -o ./common"
  },
  "packageManager": "pnpm@9.15.2+sha256.022309bb31359142b65bfa889e0406d2eebd5acfffca47e6944acf29d9d6a66b",
  "devDependencies": {
    "prettier": "^2.6.2"
  }
}
